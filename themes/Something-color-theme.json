			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5A826B"
			}
		},
		{
			"name": "Documentation Comments",
			"scope": "comment.block.documentation",
			"settings": {
				"foreground": "#B5C3C4"
			}
