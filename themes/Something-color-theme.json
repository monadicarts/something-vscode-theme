	"tokenColors": [
		
		{
			"name": "Default text and variables",
			"scope": ["variable", "meta.definition.variable.name", "support.variable", "entity.name.variable"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A9B6B7"
			}
		},
		{
			"name": "Documentation Comments",
			"scope": "comment.block.documentation",
			"settings": {
				"foreground": "#B5C3C4"
			}
		},
		{
			"name": "Documentation Tags",
			"scope": [
				"storage.type.class.jsdoc",
				"variable.other.jsdoc",
				"punctuation.definition.tag", 
				"keyword.other.phpdoc"
			],
			"settings": { "foreground": "#82AAFF", "fontStyle": "" }
		},
		{
			"name": "String Interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#00D0E6"
			}
		},
		{
			"name": "Strings",
			"scope": ["string", "punctuation.definition.string"],
			"settings": {
				"foreground": "#52D98F"
			}
		},
		{
			"name": "Invalid and Deprecated",
			"scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
			"settings": {
				"foreground": "#C99492"
			}
		},
		{
			"name": "Comment Tags (TODO, FIXME, etc.)",
			"scope": [
				"comment.keyword",
				"comment.keyword.todo",
				"keyword.other.todo",
				"meta.todo"
			],
			"settings": {
				"foreground": "#FF8DD9",
				"fontStyle": "bold"
			}
		},

		
		{
			"name": "Keywords, Storage, Modifiers",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#B07CC3"
			}
		},
		{
			"name": "Preprocessor Directives",
			"scope": [
				"keyword.control.directive",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#B07CC3"
			}
		},
		{
			"name": "Operators and Punctuation",
			"scope": [
				"keyword.operator",
				"punctuation",
				"meta.brace",
				"punctuation.terminator",
				"punctuation.separator"
			],
			"settings": {
				"foreground": "#00D0E6"
			}
		},
		{
			"name": "Embedded Language Punctuation",
			"scope": ["punctuation.section.embedded.begin", "punctuation.section.embedded.end"],
			"settings": {
				"foreground": "#00D0E6"
			}
		},
		{
			"name": "Punctuation: Accessors",
			"scope": ["punctuation.accessor"],
			"settings": {
				"foreground": "#ffffff"
			}
		},

		
		{
			"name": "Function and method names",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic",
				"entity.name.method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Function parameters",
			"scope": ["variable.parameter", "meta.function-call.arguments"],
			"settings": {
				"foreground": "#C99492",
				"fontStyle": "italic"
			}
		},

		
		{
			"name": "Types, Classes, Structs, Enums",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.struct",
				"entity.name.enum",
				"support.type",
				"support.class",
				"storage.type.cs", 
				"storage.type.java" 
			],
			"settings": {
				"foreground": "#FFDD29"
			}
		},
		{
			"name": "Modules, Namespaces, Packages",
			"scope": [
				"entity.name.namespace",
				"entity.name.package",
				"entity.name.module"
			],
			"settings": {
				"foreground": "#FFDD29"
			}
		},
		{
			"name": "Inherited Class",
			"scope": ["entity.other.inherited-class"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFDD29"
			}
		},
		{
			"name": "HTML/XML/JSX Tags",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#FFDD29"
			}
		},
		{
			"name": "Testing Keywords (Jest, Mocha, etc.)",
			"scope": [
				"entity.name.function.test",
				"support.function.test",
				"keyword.control.test"
			],
			"settings": {
				"foreground": "#52D98F"
			}
		},

		
		{
			"name": "Numbers, Booleans, Null, Built-in Constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character.numeric",
				"support.constant"
			],
			"settings": {
				"foreground": "#D9B38C"
			}
		},
		{
			"name": "Escape characters and Regex",
			"scope": ["constant.character.escape", "string.regexp"],
			"settings": {
				"foreground": "#00D0E6"
			}
		},

		
		{
			"name": "Object properties, Attribute names, Keys",
			"scope": [
				"support.type.property-name",
				"entity.other.attribute-name",
				"meta.object-literal.key",
				"support.variable.property",
				"variable.other.property",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#C99492"
			}
		},
		{
			"name": "Decorators, Annotations, Directives",
			"scope": [
				"meta.decorator",
				"entity.name.decorator",
				"storage.type.annotation",
				"punctuation.definition.annotation", 
				"punctuation.definition.entity.css",
				"keyword.control.directive"
			],
			"settings": {
				"foreground": "#FF8DD9"
			}
		},

		
		{
			"name": "Shell: Special Variables",
			"scope": ["variable.other.special.shell"],
			"settings": { "foreground": "#D9B38C" }
		},
		{
			"name": "Python: f-string interpolation",
			"scope": "constant.character.format.placeholder.other.python",
			"settings": {
				"foreground": "#00D0E6"
			}
		},

		

		
		{
			"name": "Clojure Keywords",
			"scope": ["constant.keyword.clojure"],
			"settings": {
				"foreground": "#D9B38C"
			}
		},

		
		{
			"name": "Go Types",
			"scope": ["storage.type.go"],
			"settings": {
				"foreground": "#FFDD29"
			}
		},

		
		{
			"name": "Rust Macros",
			"scope": ["support.macro.rust"],
			"settings": {
				"foreground": "#FF8DD9"
			}
		},
		{
			"name": "Rust Lifetimes",
			"scope": ["entity.name.lifetime.rust", "storage.modifier.lifetime.rust"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#D9B38C"
			}
		},

		
		{
			"name": "CSS Property Names",
			"scope": ["support.type.property-name.css"],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "CSS Units",
			"scope": ["keyword.other.unit"],
			"settings": {
				"foreground": "#D9B38C"
			}
		},
		{
			"name": "CSS Tag, Class, and ID selectors",
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#FFDD29"
			}
		},

		
		{
			"name": "JSON/YAML/TOML Keys",
			"scope": [
				"support.type.property-name.json",
				"entity.name.tag.yaml",
				"support.type.property-name.toml"
